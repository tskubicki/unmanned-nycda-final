<%= render partial: "/navbar" %>

<%= form_tag(users_path, :method => "get") do %>
      <%= text_field_tag :search, params[:search], placeholder: "Search" %>
      <%= submit_tag :search %>
<% end %>

<% @users.each do |u| %>
	<p>
		<%= image_tag u.avatar.url(:thumb) %>
		<%= u.username %>
		<%= link_to "Profile", user_path(u.id) %>
	</p>
<% end %>